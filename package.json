{
  "name": "tasteos",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev:web": "pnpm -C apps/web dev",
    "build:web": "pnpm -C apps/web build",
    "lint:web": "pnpm -C apps/web lint",
    "test:web": "pnpm -C apps/web test",
    "api:install": "python -m pip install -r services/api/requirements.txt",
    "api:dev": "cd services/api && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "api:test": "cd services/api && pytest -q"
  }
}
